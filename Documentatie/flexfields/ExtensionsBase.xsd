			<xs:element name="featureset" type="featuresettype" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="forbiddenfeaturesetstype">
		<xs:sequence>
			<xs:element name="featureset" type="featuresettype" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="azurefunctionstype">
		<xs:sequence>
			<xs:element name="function" type="functiontype" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="functiontype">
		<xs:sequence>
			<xs:element name="endpoint" type="hreftype" minOccurs="1"/>
			<xs:element name="authorization" minOccurs="1">
				<xs:complexType>
					<xs:sequence>
						<xs:choice>
							<xs:element name="keyvault">
								<xs:complexType>
									<xs:attribute name="endpoint" type="xs:string" use="required"/>
									<xs:attribute name="secret" type="xs:string" use="required"/>
								</xs:complexType>
							</xs:element>
							<xs:element name="apikey" type="xs:string"/>
						</xs:choice>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="parameters">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="parameter" minOccurs="1" maxOccurs="unbounded">
							<xs:complexType>
								<xs:attribute name="name" type="xs:string" use="required"/>
								<xs:attribute name="caption" type="xs:string" use="required"/>
								<xs:attribute name="required" type="xs:boolean" use="optional"/>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="name" type="xs:string" use="required"/>
		<xs:attribute name="caption" type="xs:string" use="required"/>
		<xs:attribute name="returntype" type="returntypetype" use="required"/>
	</xs:complexType>
	<xs:complexType name="entitiestype">
		<xs:sequence>
			<xs:element name="entity" type="entitytype" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="entitytype">
		<xs:sequence>
			<xs:element name="documentation" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="property" type="propertytype" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="name" type="nowhitespacestringtype" use="required"/>
		<xs:attribute name="table" type="sqltablecolumnlength" use="required"/>
		<xs:attribute name="extensiontable" type="sqltablecolumnlength" use="required"/>
		<xs:attribute name="businesscomponent" type="nowhitespacestringtype" use="required"/>
	</xs:complexType>
	<xs:complexType name="propertytype">
		<xs:sequence>
			<xs:element name="listitems" type="listitemstype" minOccurs="0" maxOccurs="1">
				<xs:unique name="unique_value">
					<xs:selector xpath="listitem"/>
					<xs:field xpath="@value"/>
				</xs:unique>
			</xs:element>
			<xs:element name="einvoiceelement" type="einvoiceelementtype" minOccurs="0" maxOccurs="1" />
		</xs:sequence>
		<xs:attribute name="name" type="nowhitespacestringtype" use="required"/>
		<xs:attribute name="columnname" type="sqltablecolumnlength" use="required"/>
		<xs:attribute name="caption" type="xs:string" use="required"/>
		<xs:attribute name="captionid" type="termidtype" use="optional"/>
		<xs:attribute name="translationid" type="translationidtype" use="optional"/>
		<xs:attribute name="type" type="propertytypetype" use="required"/>
		<xs:attribute name="length" use="optional">
			<xs:simpleType>
				<xs:restriction base="xs:unsignedInt">
					<xs:minInclusive value="1"/>
					<xs:maxInclusive value="8000"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="refersto" type="referstotype" use="optional"/>
		<xs:attribute name="allowempty" type="xs:boolean" use="optional"/>
		<xs:attribute name="scale" type="xs:unsignedInt" use="optional"/>
		<xs:attribute name="propertyStates" type="propertyStates" use="optional"/>
		<xs:attribute name="deleteaction" type="deleteactiontype" use="optional"/>
	</xs:complexType>
	<xs:complexType name="listitemstype">
		<xs:sequence>
			<xs:element name="documentation" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="listitem" minOccurs="1" maxOccurs="unbounded">
				<xs:complexType>
					<xs:attribute name="value" type="listitemstringtype" use="required"/>
					<xs:attribute name="caption" type="listitemstringtype" use="optional"/>
					<xs:attribute name="captionid" type="termidtype" use="optional"/>
					<xs:attribute name="translationid" type="translationidtype" use="optional"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="allowempty" type="xs:boolean" use="optional"/>
		<xs:attribute name="listdefinitionid" type="nowhitespacestringtype" use="required"/>
	</xs:complexType>
	<xs:complexType name="businesscomponentextensionstype">
		<xs:sequence>
			<xs:element name="businesscomponent" type="businesscomponenttype" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="businesscomponenttype">
		<xs:sequence>
			<xs:element name="documentation" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="azurevalidations" minOccurs="0" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="functionalcomponent" minOccurs="0" maxOccurs="unbounded">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="documentation" type="xs:string" minOccurs="0" maxOccurs="1"/>
									<xs:element minOccurs="0" maxOccurs="unbounded" name="inputproperty">
										<xs:complexType>
											<xs:attribute name="property" type="nowhitespacestringtype" use="required"/>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
								<xs:attribute name="targetproperty" type="nowhitespacestringtype" use="required"/>
								<xs:attribute name="azureoperationname" type="nowhitespacestringtype" use="required"/>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="azurefunctionendpoint" type="xs:anyURI" use="required"/>
					<xs:attribute name="azuresecretref" type="textwithdashallowed" use="optional"/>
					<xs:attribute name="apikey" type="xs:string" use="optional"/>
				</xs:complexType>
				<!-- Validate either azuresecretref or apikey attribute specified (but not both). -->
				<xs:key name="AzureSecretRefOrApiKey">
					<xs:selector xpath="."/>
					<xs:field xpath="@azuresecretref | @apikey"/>
				</xs:key>
			</xs:element>
			<xs:element name="references" minOccurs="0" maxOccurs="1">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="reference" minOccurs="0" maxOccurs="unbounded">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="property" minOccurs="0" maxOccurs="unbounded">
										<xs:complexType>
											<xs:sequence>
											</xs:sequence>
											<xs:attribute name="name" type="nowhitespacestringtype" use="required"/>
											<xs:attribute name="columnname" type="nowhitespacestringtype" use="required"/>
											<xs:attribute name="caption" type="xs:string" use="optional"/>
											<xs:attribute name="captionid" type="termidtype" use="optional"/>
											<xs:attribute name="translationid" type="translationidtype" use="optional"/>
											<xs:attribute name="domainid" type="idguidtype" use="optional"/>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
								<xs:attribute name="forproperty" type="nowhitespacestringtype" use="required"/>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="functionalcomponent" minOccurs="0" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence maxOccurs="1" minOccurs="0">
						<xs:element name="documentation" type="xs:string" minOccurs="0" maxOccurs="1"/>
						<xs:element name="expression" type="xs:string"/>
					</xs:sequence>
					<xs:attribute name="type" type="custompropertyrelationtype" use="required"/>
					<xs:attribute name="targetproperty" type="nowhitespacestringtype" use="required"/>
					<xs:attribute name="class" type="assemblyorclassnametype" use="optional"/>
					<xs:attribute name="assembly" type="assemblyorclassnametype" use="optional"/>
					<xs:attribute name="version" type="majorminorpatchversiontype" use="optional"/>
					<xs:attribute name="propertyStates" type="propertyStates" use="optional"/>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="name" type="nowhitespacestringtype" use="required"/>
	</xs:complexType>
	<xs:complexType name="megamenuextensionstype">
		<xs:sequence>
			<xs:element name="megamenuextension" type="megamenuextensiontype" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="megamenuextensiontype">
		<xs:sequence>
			<xs:element minOccurs="0" maxOccurs="unbounded" name="tab">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="mandatoryfeaturesets" type="mandatoryfeaturesetstype" minOccurs="0" maxOccurs="1"/>
						<xs:element name="forbiddenfeaturesets" type="forbiddenfeaturesetstype" minOccurs="0" maxOccurs="1"/>
						<xs:element name="featurecheck" type="featurechecktype" minOccurs="0" maxOccurs="1"/>
						<xs:element name="mandatorylegislation" type="legislationtype" minOccurs="0" maxOccurs="1"/>
						<xs:element name="visibleexpression" minOccurs="0" maxOccurs="1"/>
						<xs:element name="functionpoint" type="xs:string" minOccurs="0" maxOccurs="1"/>
						<xs:element name="permission" type="permissiontype" minOccurs="0" maxOccurs="1"/>
						<xs:element name="documentation" type="xs:string" minOccurs="0" maxOccurs="1"/>
						<xs:element name="section" minOccurs="0" maxOccurs="unbounded">
