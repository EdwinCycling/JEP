<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:simpleType name="idguidtype">
		<xs:restriction base="xs:string">
			<xs:pattern value="[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="idtype">
		<xs:restriction base="xs:string">
			<xs:pattern value="[A-Za-z_]([A-Za-z0-9_])*"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="languagecode">
		<xs:restriction base="xs:string">
			<xs:pattern value="[a-z]{2}(-[A-Z]{2})?"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="textwithdotallowed">
		<xs:restriction base="xs:string">
			<xs:pattern value="[\w.]*"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="textwithdashallowed">
		<xs:restriction base="xs:string">
			<xs:pattern value="[\w-]*"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="assemblyorclassnametype">
		<xs:restriction base="xs:string">
			<xs:pattern value="[^.]([A-Za-z0-9_.])*[^.]"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="majorminorpatchversiontype">
		<xs:restriction base="xs:string">
			<xs:pattern value="([0-9]+)\.([0-9]+)\.([0-9]+)"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="nowhitespacestringtype">
		<xs:restriction base="xs:string">
			<xs:pattern value="[A-Za-z_]([A-Za-z0-9_])*"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="termidtype">
		<xs:restriction base="xs:unsignedInt"/>
	</xs:simpleType>
	<xs:simpleType name="featuresettype">
		<xs:restriction base="xs:string">
			<xs:pattern value="[A-Za-z_]([A-Za-z0-9_])*"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="extensioncodetype">
		<xs:restriction base="xs:string">
			<xs:pattern value="[A-Z]{3,5}_[_A-Za-z0-9]+"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="applicationtype">
		<xs:restriction base="xs:string">
			<xs:pattern value="([A-Za-z0-9_.-])+\.aspx"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="hreftype">
		<xs:restriction base="xs:string">
			<!-- 
				Allows http:// or https:// 
				Domain address can have colon : or underscore _
				Special characters allowed after the hostname are as below.
					 @%_+.~#?;&/={}()',
				Other special characters must be escaped.
				Disallows name with @ before the domain address (e.g. http://myName@www.google.com)
			-->
			<xs:pattern value="((http(s)?:\/\/[-a-zA-Z0-9._]{1,256}(:[0-9]{2,6})?(\/?[-a-zA-Z0-9%_\+.~#?;&amp;={}()',&lt;&gt;/:]*)?)|(\/?[-a-zA-Z0-9%_\+.~#;&amp;={}()',&lt;&gt;/]+(\?[-a-zA-Z0-9%_\+.~#;&amp;={}()',&lt;&gt;/:]*)?)?)"/>
		</xs:restriction>
	</xs:simpleType>
